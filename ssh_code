sudo apt-get update
sudo apt-get upgrade
cd /usr/local/src
sudo wget https://dl.ui.com/unifi/5.12.35/unifi_sysvinit_all.deb
sudo dpkg -i unifi_sysvinit_all.deb
sudo apt-get install -f
sudo apt-get install openjdk-8-jre

echo "JAVA_HOME=/usr/lib/jvm/java-8-openjdk-armhf" | sudo tee -a /etc/default/unifi
JAVA_HOME=/usr/lib/jvm/java-8-openjdk-armhf

or

wget "https://github.com/SmokingCrop/UniFi/raw/master/install-unifi-pihole-English.sh" -O install-unifi-pihole.sh && chmod +x install-unifi-pihole.sh && ./install-unifi-pihole.sh no-pihole


sudo service unifi status
